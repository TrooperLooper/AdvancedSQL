  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch index.ts",
    "start": "tsx index.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:backup": "docker exec postgres_container pg_dump -U root test_db > backup.sql",
    "db:restore": "docker exec -i postgres_container psql -U root test_db < backup.sql",
    "db:reset": "docker exec postgres_container psql -U root -c 'DROP DATABASE IF EXISTS test_db; CREATE DATABASE test_db;' && npm run db:restore"
  },
